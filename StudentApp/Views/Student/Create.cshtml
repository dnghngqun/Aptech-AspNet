@model StudentApp.Models.Student

<h1>Create Student</h1>
<form method="post">
    Name: <input type="text" name="Name" required placeholder="Enter name"> <br>
    Age: <input type="number" name="Age" required placeholder="Enter Age"> <br>
    Email: <input type="email" name="Email" placeholder="Enter email" required><br>
    <button type="submit">Create</button>
</form>